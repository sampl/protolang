{
  "name": "languages-fe",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "spellcheck": "cspell --config cspell.json \"**\"",
    "db:clear": "node scripts/clear.js",
    "db:migrate": "node scripts/migrate.js --new",
    "db:seed": "node scripts/seed.js",
    "db:reset": "npm run db:clear && npm run db:migrate && npm run db:seed",
    "db:backup": "node scripts/backup.js",
    "update:dictionary": "node scripts/updates/dictionary.mjs",
    "update:lessons": "node scripts/updates/lessons.mjs",
    "update:mnemonics": "node scripts/updates/mnemonics.mjs",
    "update:phrases": "node scripts/updates/phrases.mjs",
    "update:all": "npm run update:lessons -- live && npm run update:mnemonics -- live && npm run update:phrases -- live",
    "use:dev": "supabase link --project-ref ADD_PROJECT_ID_HERE",
    "use:prod": "supabase link --project-ref ADD_PROJECT_ID_HERE",
    "functions:serve": "supabase functions serve --env-file .env.development",
    "functions:setSecrets:dev": "supabase secrets set --env-file .env.development",
    "functions:setSecrets:prod": "supabase secrets set --env-file .env.production",
    "functions:deploy:dev": "env-cmd -f .env.development supabase functions deploy hello --project-ref ADD_PROJECT_ID_HERE",
    "functions:deploy:prod": "env-cmd -f .env.production supabase functions deploy hello --project-ref ADD_PROJECT_ID_HERE"
  },
  "dependencies": {
    "@formspree/react": "^2.4.1",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-popover": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.1",
    "@sentry/react": "^7.40.0",
    "@sentry/tracing": "^7.40.0",
    "@supabase/supabase-js": "^2.8.0",
    "diff": "^5.1.0",
    "dotenv": "^16.0.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-textarea-autosize": "^8.4.0",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "remark-directive-rehype": "^0.4.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-parse-yaml": "^0.0.3",
    "remark-stringify": "^10.0.2",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.6",
    "use-debounce": "^9.0.3",
    "use-timer": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cspell": "^6.21.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.13.4",
    "line-by-line": "^0.1.6",
    "pg": "^8.9.0",
    "pg-promise": "^10.11.1",
    "vite": "^2.9.9",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-markdown": "^2.1.0"
  }
}
